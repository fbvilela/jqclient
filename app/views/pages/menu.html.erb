<% render partial: "shared/flash_messages"%>


       
    <!-- Top Navbar-->
		<div class="navbar">
		  <div class="navbar-inner">
		    <div class="center sliding">iDashcontacts</div>
		  </div>
		</div>

    <!-- Pages, because we need fixed-through navbar and toolbar, it has additional appropriate classes-->
    <div class="pages navbar-through toolbar-through">
      <!-- Page, data-page contains page name-->
      <div data-page="index" class="page page-homepage no-toolbar">
        <!-- Scrollable page content-->
        <div class="page-content">
          
          <ul>
	          <li>
		          <%= link_to contacts_vendors_path do %>
			          <p>
				          <i class="i-flag i-flag-sale"></i>
				          <br>
				          My vendors
			          </p>
              <% end %>
	          </li>
	          <li>
		          <%= link_to contacts_landlords_path do %>
			          <p>
				          <i class="i-flag i-flag-rent"></i>
				          <br>
				          My landlords
			          </p>
              <% end %>
	          </li>
	          <li>
		          <a href="/contacts">
			          <p>
				          <i class="i-address-book"></i>
				          <br>
				          My contacts
			          </p>
		          </a>
	          </li>
	          <li>
		          <a href="/contacts/new">
			          <p>
				          <i class="i-plus"></i>
				          <br>
				          Add contact
			          </p>
		          </a>
	          </li>
	          <li>
		          <a>
			          <p>
				          <i class="i-home"></i>
				          <br>
				          My Listings
			          </p>
		          </a>
	          </li>
	          <li>
		          <a>
			          <p>
				          <i class="i-sun"></i>
				          <br>
				          My day
			          </p>
		          </a>
	          </li>
          </ul>
          
        </div><!-- page content -->
      </div><!-- page -->
    </div><!-- pages -->