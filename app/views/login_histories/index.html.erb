<h1>Listing login_histories</h1>

<table>
  <tr>
    <th>User</th>
    <th>Last Login</th>
    <th>Count</th>
  </tr>

<% @count_hash.each_key do |k| %>


  <tr>
		<td><%= k %></td>
		<td><%= LoginHistory.where("login = ? ", k).order("created_at desc").limit(1).first.name %></td>
		<td><%= @count_hash[k] %></td>
    
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Login history', new_login_history_path %>
